  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@nuxt/icon": "^2.1.0",
    "@nuxt/image": "^2.0.0",
    "@nuxt/kit": "^4.2.0",
    "@nuxt/ui": "^4.1.0",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/robots": "^5.5.6",
    "@nuxtjs/sitemap": "^7.4.7",
    "@types/luxon": "^3.7.1",
    "luxon": "^3.7.2",
    "nuxt": "^4.2.0",
    "nuxt-viewport": "^2.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@iconify-json/cib": "^1.2.3",
    "@iconify-json/lucide": "^1.2.73",
    "@iconify-json/material-symbols": "^1.2.46",
    "@iconify-json/mdi": "^1.2.3",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@types/bun": "latest",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
